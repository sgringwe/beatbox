public class BeatBox.TrackInfo : BasicInfo {
	public string title { get; set; }
	public string artist { get; set; }
	public string lyrics { get; set; }
	
	public TrackInfo() {
		
	}
	
	public void merge_info(TrackInfo other) {
		merge_basic_info(other);
		
		if(String.is_empty(lyrics)) {
			lyrics = other.lyrics;
		}
	}
}
